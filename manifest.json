{
  "manifest_version": 3,
  "name": "Word Wise Quotes",
  "description": "Get motivational quotes right in your Chrome extension popup!",
  "version": "1.0",
  "permissions": ["storage", "clipboardWrite"],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
